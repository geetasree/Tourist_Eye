<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true">
        <shape>
            <solid
                android:color="#99d4f2" />
            <stroke
                android:width="1dp"
                android:color="#ffffff" />
            <corners
                android:radius="12dp" />
            <padding
                android:left="12dp"
                android:top="6dp"
                android:right="6dp"
                android:bottom="6dp" />
        </shape>

    </item>



</selector>